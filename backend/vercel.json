{
    "version": 2,
    "builds": [
     {
         "src": "./index.js",
         "use": "@vercel/node"
     }
    ],
    "routes": [
     {
         "src": "/(.*)",
         "dest": "/"
     }
    ],
    "crons": [
        {
            "path": "/email.js",
            "schedule": "*/2 * * * *"
        }

    ]
 }